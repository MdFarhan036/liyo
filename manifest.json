{
    "manifest_version": 3,
    "name": "Liyo",
    "description": "like and comment in random linkdin posts ",
    "version": "1.0.0",
    "icons": {
        "128": "Logo.png"
    },
    "permissions": ["tabs", "activeTab", "scripting", "storage"],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "action": {
        "default_popup": "popup.html"
    }
}